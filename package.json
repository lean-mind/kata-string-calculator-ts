{
  "name": "kata-string-calculator",
  "version": "1.0.0",
  "description": "Kata string calculator based on https://kata-log.rocks/string-calculator-kata",
  "main": "build/index.js",
  "files": [
    "build"
  ],
  "scripts": {
    "build": "npm run lint && npm test && tsc",
    "lint": "eslint . --ext .ts",
    "lint:fix": "npm run lint -- --fix",
    "test": "jest --verbose",
    "test:watch": "npm t -- --watchAll",
    "test:coverage": "npm t -- --coverage",
    "git:red": "git add . && git commit -m \"🧪 Failing test\" && git push",
    "git:green": "git add . && git commit -m \"✅ Pass test\" && git push",
    "git:refactor": "git add . && git commit -m \"♻️ Refactor test\" && git push"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lean-mind/kata-string-calculator-ts.git"
  },
  "homepage": "https://github.com/lean-mind/kata-string-calculator-ts#readme",
  "author": "Lean Mind <contacto@leanmind.es>",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "26.0.5",
    "@typescript-eslint/eslint-plugin": "4.16.1",
    "@typescript-eslint/parser": "4.16.1",
    "eslint": "7.21.0",
    "eslint-config-standard": "16.0.2",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "4.3.1",
    "jest": "26.1.0",
    "lint-staged": "10.2.11",
    "ts-jest": "26.1.3",
    "typescript": "4.2.4"
  }
}
